{
    "name": "keltron/filehelper",
    "description": ":Filehelper for Keltron inhouse projects",
    "license": "",
    "authors": [
        {
            "name": "KELTRON",
            "email": "hareendran@keltron.org",
            "homepage": "https://github.com/hareendranmg/filehelper"
        }
    ],
    "homepage": "https://github.com/hareendranmg/filehelper",
    "keywords": ["Laravel", "Filehelper"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Keltron\\Filehelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keltron\\Filehelper\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Keltron\\Filehelper\\FilehelperServiceProvider"
            ],
            "aliases": {
                "Filehelper": "Keltron\\Filehelper\\Facades\\Filehelper"
            }
        }
    }
}
